<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
    <%= content_tag_for(:tr, link) do %>
      <%= render partial: 'link_row', locals: {link: link} %>
      <% end %>
    <% end %>
  </tbody>
</table>
<%= paginate @links %>
<br>

<%= link_to 'New Link', new_link_path %>
